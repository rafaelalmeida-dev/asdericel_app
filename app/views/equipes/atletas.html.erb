<%= render "shared/context_nav", title: t("activerecord.models.equipe.other") do  %>
  <%= content_tag(:span, link_to(t("activerecord.models.equipe.other"), equipes_path, class: "text-dark"), class: "breadcrumb-item") %>
  <%= content_tag(:span, t("common.actions.#{params[:action]}"),class: "breadcrumb-item active")%>
<% end %>
<%= simple_form_for @equipe, url: atletas_equipe_path(@equipe), method: :post do |f| %>
  <!-- Área de notificações -->
  <%= render "shared/model_errors" %>
  <%= render "shared/notice" %>
  <!-- Card e formulário -->
  <div class="card">
    <div class="card-header">
      <h5 class="my-0">
        <%= translate_concat("common.actions.#{params[:action]}") %>
      </h5>
    </div>
    <div class="card-body">
      <form>
        <%= link_to t("common.back"), equipes_path, class: "btn btn-outline-secondary" %>
      </form>
    </div>
  </div>
<% end %>